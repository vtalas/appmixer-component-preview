{
	"name": "appmixer-component-preview",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev --port 5151",
		"build": "npm run generate-data && vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"generate-data": "node scripts/generate-data.js",
		"tauri": "PATH=$HOME/.cargo/bin:$PATH tauri",
		"tauri:dev": "PATH=$HOME/.cargo/bin:$PATH tauri dev",
		"tauri:build": "PATH=$HOME/.cargo/bin:$PATH tauri build"
	},
	"devDependencies": {
		"@internationalized/date": "^3.10.1",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.50.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/vite": "^4.1.18",
		"@tauri-apps/cli": "^2.10.0",
		"svelte": "^5.48.2",
		"svelte-check": "^4.3.5",
		"tailwindcss": "^4.1.18",
		"vite": "^7.3.1"
	},
	"dependencies": {
		"@sveltejs/adapter-node": "^5.5.2",
		"@sveltejs/adapter-static": "^3.0.10",
		"@tauri-apps/api": "^2.10.1",
		"@tauri-apps/plugin-dialog": "^2.6.0",
		"@tauri-apps/plugin-fs": "^2.4.5",
		"@tauri-apps/plugin-shell": "^2.3.5",
		"bits-ui": "^2.15.4",
		"clsx": "^2.1.1",
		"lucide-svelte": "^0.563.0",
		"react": "^19.2.4",
		"react-dom": "^19.2.4",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.2.2",
		"vibe-kanban-web-companion": "^0.0.5"
	}
}
